{
  "name": "@catppuccin/vscode-monorepo",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.18.13",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,json,md}": "prettier --write"
  },
  "scripts": {
    "lint": "eslint . --ext .ts && yarn typecheck:all",
    "postinstall": "husky install",
    "typecheck:all": "tsc --noEmit -p packages/catppuccin-vsc && tsc --noEmit -p packages/catppuccin-vsc-storybook"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.0.2"
}
