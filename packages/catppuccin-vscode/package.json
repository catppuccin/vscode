{
  "name": "@catppuccin/vscode",
  "version": "3.19.0",
  "description": "ðŸ¦Œ Soothing pastel theme for VSCode, NPM version",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catppuccin/vscode.git",
    "directory": "packages/catppuccin-vscode"
  },
  "files": [
    "dist/*",
    "themes/*"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./themes/*.json": {
      "import": "./themes/*.json",
      "require": "./themes/*.json"
    }
  },
  "dependencies": {
    "@catppuccin/palette": "catalog:"
  },
  "devDependencies": {
    "@tsconfig/node22": "catalog:",
    "@types/node": "catalog:",
    "catppuccin-vsc": "workspace:*",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "pnpm tsup",
    "compiled:pack": "pnpm pack",
    "prepack": "pnpm --filter catppuccin-vsc core:build && pnpm build"
  },
  "homepage": "https://github.com/catppuccin/vscode/tree/main/packages/catppuccin-vscode#readme",
  "bugs": {
    "url": "https://github.com/catppuccin/vscode/issues"
  },
  "sponsor": {
    "url": "https://opencollective.com/catppuccin"
  }
}
