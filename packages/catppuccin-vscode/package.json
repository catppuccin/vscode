{
  "name": "@catppuccin/vscode",
  "version": "3.15.0",
  "description": "ðŸ¦Œ Soothing pastel theme for VSCode, NPM version",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catppuccin/vscode.git",
    "directory": "packages/catppuccin-vscode"
  },
  "files": [
    "dist/*",
    "themes/*"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./themes/*.json": {
      "import": "./themes/*.json",
      "require": "./themes/*.json"
    }
  },
  "dependencies": {
    "@catppuccin/palette": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "catppuccin-vsc": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "yarn tsup",
    "compiled:pack": "npm pack",
    "prepack": "yarn core:build && yarn build"
  },
  "homepage": "https://github.com/catppuccin/vscode/tree/main/packages/catppuccin-vscode#readme",
  "bugs": {
    "url": "https://github.com/catppuccin/vscode/issues"
  },
  "sponsor": {
    "url": "https://opencollective.com/catppuccin"
  }
}
